.list {
    display: flex;
    justify-content: space-between;
    font-size: 16px;
    padding: 10px;
    border-bottom: 1px solid #bfbfbf;
}

.empty {
    text-align: center;
    margin-top: 30px;
    color: #bfbfbf;
}

.empty image {
    width: 20vw;
    height: 20vw;
}

.empty .btn {
    width: 30vw;
    height: 30px;
    line-height: 30px;
    background: #f8441f;
    color: #fff;
    margin-left: 35vw;
    border-radius: 30px;
    margin-top: 20px;
    font-size: 14px;
}
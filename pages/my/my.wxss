.head {
    width: 100%;
    height: 100px;
    text-align: center;
}

.list {
    display: flex;
    justify-content: space-between;
    font-size: 16px;
    padding: 10px;
    border-bottom: 1px solid #bfbfbf;
}

.list image {
    width: 20px;
    height: 20px;
}

.kf {
    width: 30px;
    height: 30px;
    position: fixed;
    right: 20px;
    top: 60%;
    padding: 5px;
    background: rgba(0,0,0,.2);
    border-radius: 50%;
}
page {
    background: #f4f4f4;
    font-size: 14px;
}

.checkbox {
    display: flex;
    justify-content: space-around;
    width: 90%;
    margin: 4%;
    padding: 1%;
    background: #fff;
    border-radius: 10px;
    min-height: 80px;
}

checkbox {
    margin-top: 7vw;
}

.btnlist {
    display: flex;
    margin-top: 7px;
    height: 40px;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
}

.btnlist .left {
    background: #fe9005;
}

.btnlist .left,.btnlist .right {
    color: #fff;
    height: 40px;
    line-height: 40px;
    width: 50%;
    text-align: center;
}

.btnlist .right {
    background: #fc5900;
}
<uni-base-page class="system-info-page" vue-id="28765701-1" bind:__l="__l" vue-slots="{{['default']}}"><view class="system-info-content"><block wx:if="{{devObj.category===7}}"><view class="top-item-container"><block wx:if="{{devObj.category===7}}"><view class="system-info-item"><text class="left">{{manage["protocol"]||"韦根协议"}}</text><text class="right">{{devObj.wiegand||26}}</text></view></block></view></block><block wx:else><view class="top-item-container"><view class="system-info-item"><text class="left">{{manage["open-door-time"]||"开门时长"}}</text><text class="right">{{devObj.openDelay||5}}</text></view><view class="system-info-item"><text class="left">{{manage["card-num"]||"卡登记数"}}</text><text class="right">{{devObj.regCardNums||0}}</text></view><view class="system-info-item"><text class="left">{{manage["phone-num"]||"手机登记数"}}</text><text class="right">{{devObj.regPhoneNums||0}}</text></view><view class="system-info-item"><text class="left">{{manage["maxium"]||"最大用户容量"}}</text><text class="right">{{devObj.maxContainer||1000}}</text></view><view class="system-info-item"><text class="left">{{manage["dev-switch"]||"设备开关控制信号"}}</text><text class="right">{{devControlType[devObj.controlWay]||'电锁控制'}}</text></view></view></block><block wx:if="{{devObj.deviceDoorNo!==undefined}}"><view class="system-info-item"><text class="left">{{manage["dev-door-number"]||"设备门编号"}}</text><text class="right">{{devObj.deviceDoorNo}}</text></view></block><block wx:if="{{devObj.mifareSector!==undefined}}"><view class="system-info-item"><text class="left">{{manage["dev-sector"]||"设备扇区"}}</text><text class="right">{{devObj.mifareSector}}</text></view></block><block wx:if="{{devObj.sectorKey!==undefined}}"><view class="system-info-item"><text class="left">{{manage["dev-sector-key"]||"设备扇区密钥"}}</text><text class="right">{{devObj.sectorKey}}</text></view></block><block wx:if="{{devObj.serverIp!==undefined&&devObj.serverPort!==undefined}}"><view class="system-info-item"><text class="left">{{manage["sever-ip"]||"服务器地址"}}</text><text class="right">{{devObj.serverIp+":"+devObj.serverPort}}</text></view></block><block wx:if="{{devObj.wifiName!==undefined}}"><view class="system-info-item"><text class="left">{{manage["wifi-name"]||"Wifi名称"}}</text><text class="right">{{devObj.wifiName}}</text></view></block><block wx:if="{{devObj.wifiPwd!==undefined}}"><view class="system-info-item"><text class="left">{{manage["wifi-pwd"]||"Wifi密码"}}</text><text class="right">{{devObj.wifiPwd}}</text></view></block><block wx:if="{{devObj.deviceDate!==undefined}}"><view class="system-info-item"><text class="left">{{manage["dev-time"]||"设备时间"}}</text><text class="right">{{devObj.deviceDate}}</text></view></block><block wx:if="{{devObj.electricity!==undefined}}"><view class="system-info-item"><text class="left">{{manage["dev-electricity"]||"设备电量"}}</text><text class="right">{{devObj.electricity}}</text></view></block><block wx:if="{{devObj.isSupportGetNetSignal===1}}"><view class="system-info-item"><text class="left">{{manage["signal-value"]||"联网信号值"}}</text><text class="right">{{signalValue}}</text></view></block><view class="system-info-item"><text class="left">{{manage["dev-version"]||"设备版本信息"}}</text><text class="right">{{devObj.systemVersion||1}}</text></view><view class="retry-get-btn-view"><button class="button primary" type="primary" formType="submit" data-event-opts="{{[['tap',[['getDeviceConfig',['$event']]]]]}}" bindtap="__e">{{manage["re-get"]||'重新获取设备信息'}}</button></view></view></uni-base-page>
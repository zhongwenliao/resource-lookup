<uni-base-page class="uni-base-page" vue-id="e8622b02-1" bind:__l="__l" vue-slots="{{['default']}}"><view class="pages-container"><view class="card-box-view"><button data-event-opts="{{[['tap',[['syncServerCardData',['$event']]]]]}}" class="card-view" bindtap="__e"><text class="operation-title">{{manage["sync-server-data"]||"同步服务器卡数据"}}</text></button><view class="operation-instruction">{{manage["sync-card-data-tips"]||"从服务器获取卡号，进行批量删卡添卡操作，此过程可能耗时较长"}}</view></view><block wx:if="{{devObj.btCardAddType!==0}}"><view class="card-box-view"><button data-event-opts="{{[['tap',[['swipeCardAdd',['$event']]]]]}}" class="card-view" bindtap="__e"><block wx:if="{{devObj.deviceModelName==='M160'}}"><text class="operation-title">{{manage["swipe-to-report-loss"]||"刷卡挂失"}}</text></block><block wx:else><text class="operation-title">{{swipe_card_add}}</text></block></button><block wx:if="{{exitBthShow1}}"><button data-event-opts="{{[['tap',[['exitCardAdd',['$event']]]]]}}" class="exit-btn" bindtap="__e"><text class="operation-title">{{mine["logout"]||"退出"}}</text></button></block><view class="operation-instruction">{{swipe_card_add_tips}}</view></view></block><block wx:if="{{devObj.btCardAddType!==0}}"><view class="card-box-view"><button data-event-opts="{{[['tap',[['swipeCardDelete',['$event']]]]]}}" class="card-view" bindtap="__e"><block wx:if="{{devObj.deviceModelName==='M160'}}"><text class="operation-title">{{manage["swipe-to-unlock"]||"刷卡解挂"}}</text></block><text class="operation-title">{{swipe_card_delete}}</text></button><block wx:if="{{exitBthShow2}}"><button data-event-opts="{{[['tap',[['exitCardDelete',['$event']]]]]}}" class="exit-btn" bindtap="__e"><text class="operation-title">{{mine["logout"]||"退出"}}</text></button></block><view class="operation-instruction">{{swipe_card_delete_tips}}</view></view></block><view class="card-box-view"><button data-event-opts="{{[['tap',[['goToInputCard',[0]]]]]}}" class="card-view" bindtap="__e"><text class="operation-title">{{input_card_add}}</text></button><view class="operation-instruction">{{input_card_add_tips}}</view></view><view class="card-box-view"><button data-event-opts="{{[['tap',[['goToInputCard',[1]]]]]}}" class="card-view" bindtap="__e"><text class="operation-title">{{input_card_delete}}</text></button><view class="operation-instruction">{{input_card_delete_tips}}</view></view><view class="card-box-view"><button data-event-opts="{{[['tap',[['cleanCard',['$event']]]]]}}" class="card-view" bindtap="__e"><text class="operation-title">{{manage["clean-card"]||"清空设备卡数据"}}</text></button><view class="operation-instruction">{{manage["clean-card-tips"]||"删除设备所有卡号"}}</view></view><block wx:if="{{devObj.isSupportFingerprint===1}}"><view class="card-box-view"><button data-event-opts="{{[['tap',[['cleanFingerprint',['$event']]]]]}}" class="card-view" bindtap="__e"><text class="operation-title">{{manage["clean-fingerprint"]||"清空设备指纹数据"}}</text></button><view class="operation-instruction">{{manage["clean-fingerprint-tips"]||"删除设备所有指纹数据"}}</view></view></block><view class="card-box-view"><button data-event-opts="{{[['tap',[['getReadCardList',['$event']]]]]}}" class="card-view" bindtap="__e"><text class="operation-title">{{manage["read-card-list"]||"读取设备卡列表"}}</text></button><view class="operation-instruction">{{manage["read-card-list-tips"]||"读取设备上已有卡，此过程耗时较长"}}</view></view></view></uni-base-page>
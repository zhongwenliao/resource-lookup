.notice {
  width: 100%;
  padding: 0 20rpx;
  margin-top: 30rpx;
}
.notice .yuan {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}
.notice .u-card {
  margin: 0 !important;
}
.notice .u-card:after {
  border: 0;
}
.notice .u-card .content-notice .u-cell {
  padding: 15rpx 0rpx 15rpx 20rpx !important;
}
.notice .u-card .head_group .u-cell {
  padding: 20rpx 0rpx 20rpx 12rpx;
}
.notice .u-card .head_group .u-cell__left-icon-wrap {
  color: #70A4FD;
}
.notice .u-card .head_group .u-icon--right {
  size: 29rpx;
}
.notice .u-card .head_group .u-cell_title {
  font-size: 34rpx;
  font-weight: bold;
  color: #333333;
}
.notice .u-card .head_group .u-cell__value {
  font-size: 30rpx;
  color: #666666;
}
.notice .u-card .u-card__head {
  padding: 0 !important;
}
.notice .u-card .u-card__body {
  padding: 0 !important;
}
.notice .u-card .u-card__body .u-cell {
  padding: 20rpx;
}
.notice .u-card .u-card__body .u-cell .u-cell_title {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
 .u-indicator-item-round {
  background-color: #FFFFFF;
}
.grid-box {
  border-radius: 20rpx;
  font-size: 26rpx;
  color: #333333;
  overflow: hidden;
  background-color: #FFFFFF;
}
.grid {
  width: 100%;
  padding: 0 20rpx;
}
.userImage {
  margin: 0 20rpx;
  background: linear-gradient(#709BFB, #7CA3FB);
  box-shadow: 0px 10rpx 20rpx 0px rgba(83, 134, 248, 0.3);
  border-radius: 20px;
}
.bg-box {
  width: 100%;
  overflow: hidden;
}
.bg {
  width: 104%;
  position: relative;
  left: -2%;
  height: 400rpx;
  border-radius: 0 0 64% 64% / 20%;
  background: linear-gradient(#709BFB, #ADC5FA) !important;
}
.content {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #f9f9f9;
}
.logo {
  height: 200rpx;
  width: 200rpx;
  margin-top: 200rpx;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 50rpx;
}
.text-area {
  display: flex;
  justify-content: center;
}
.title {
  font-size: 36rpx;
  color: #8f8f94;
}


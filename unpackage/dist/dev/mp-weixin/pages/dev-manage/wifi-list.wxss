.uni-list-cell-navigate {
  font-size: 30rpx;
  padding: 22rpx 30rpx;
  line-height: 48rpx;
  position: relative;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  flex: 1;
  justify-content: space-between;
  align-items: center;
}
.uni-list-cell-navigate {
  padding-right: 36rpx;
}
.uni-navigate-badge {
  padding-right: 50rpx;
}
.uni-list-cell-navigate.uni-navigate-right:after {
  font-family: uniicons;
  content: '\e583';
  position: absolute;
  right: 24rpx;
  top: 50%;
  color: #bbb;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
.uni-list-cell-navigate.uni-navigate-bottom:after {
  font-family: uniicons;
  content: '\e581';
  position: absolute;
  right: 24rpx;
  top: 50%;
  color: #bbb;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
.uni-list-cell-navigate.uni-navigate-bottom.uni-active::after {
  font-family: uniicons;
  content: '\e580';
  position: absolute;
  right: 24rpx;
  top: 50%;
  color: #bbb;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
.uni-collapse.uni-list-cell {
  flex-direction: column;
}
.uni-list-cell-navigate.uni-active {
  background: #eee;
}
.input-group {
  background-color: #ffffff;
  margin-top: 40rpx;
  position: relative;
}
.input-group::before {
  position: absolute;
  right: 0;
  top: 0;
  left: 0;
  height: 1rpx;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.input-group::after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 1rpx;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.input-row {
  display: flex;
  flex-direction: row;
  position: relative;
}
.input-row .title {
  width: 20%;
  height: 50rpx;
  min-height: 50rpx;
  padding: 15rpx 0;
  padding-left: 30rpx;
  line-height: 50rpx;
}
.input-row.border::after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 15rpx;
  height: 1rpx;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #c8c7cc;
}
.btn-row {
  margin-top: 50rpx;
  padding: 20rpx;
}
button.primary {
  background-color: #0FAEFF !important;
}
text.primary {
  color: #0FAEFF !important;
}
view.green-bg-color, text.green-bg-color, button.green-bg-color {
  background-color: #09BB07 !important;
}
view.green-fg-color, text.green-fg-color, button.green-fg-color {
  color: #09BB07 !important;
}
.left {
  float: left;
}
.right {
  float: right;
}
.form {
  width: auto;
}
.form .input-view .input-row {
  border-bottom: 1rpx solid #cccccc;
}
.form .input-view .input-row .uni-input {
  height: 36rpx;
  padding: 20rpx 0;
  font-size: 30rpx;
  line-height: 36rpx;
}
.button {
  border-radius: 60rpx;
  letter-spacing: 6rpx;
}
.navigator-common {
  display: block;
  padding: 20rpx;
}
.list-view {
  width: 100%;
  background: #eeeeee;
}
.list-view .search-view {
  background: #eeeeee;
  line-height: 1;
  position: fixed;
  z-index: 1000;
  width: 100%;
  height: 100rpx;
}
.list-view .search-view .search-form {
  width: calc(100% - 60rpx);
  margin: 20rpx 30rpx;
  display: inline-block;
  background: #ffffff;
  border-radius: 10rpx;
  height: 60rpx;
}
.list-view .search-view .search-form .filter-view {
  posotion: relative;
}
.list-view .search-view .search-form .filter-view .filter-picker {
  top: 20rpx;
  height: 60rpx;
  padding: 8rpx 14rpx;
  border: 1rpx solid #eeeeee;
  border-radius: 10rpx;
  position: absolute;
  width: 90%;
}
.list-view .search-view .search-form .filter-view .filter-picker .filter-view-down-image {
  width: 40rpx;
  height: 30rpx;
  top: 20rpx;
  left: 90%;
  position: absolute;
}
.list-view .search-view .search-form .filter-view .filter-picker .input-text {
  white-space: nowrap;
  max-width: 90%;
  display: inline-block;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
  height: 60rpx;
}
.list-view .search-view .search-form .filter-view .filter-input {
  top: 24rpx;
  position: absolute;
  margin-left: 42%;
  font: inherit;
  display: inline-block;
  width: 40%;
}
.list-view .search-view .search-form .search-button::after {
  border: none;
}
.list-view .search-view .search-form .search-button {
  display: inline;
  background: none;
  margin: -20rpx 0 0 100%;
  padding: 12rpx 20rpx;
  float: left;
}
.list-view .search-view .search-form .search-image {
  width: 40rpx;
  margin: 0;
  height: 40rpx;
}
.list-view .no-data-view {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
  color: #959595;
}
.list-view .loading {
  padding: 30rpx 0;
  color: #959595;
  text-align: center;
}
.uni-base-page {
  width: 100%;
  height: 100%;
}
.red-help-text {
  color: red;
}
.bottom-btn-view {
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 100000000;
}
.btn {
  border-radius: 0px;
  font-size: 32rpx;
}
.bottom-btn-view .btn::after {
  border-radius: 0;
}
/* form视图必填 */
.form-view .require-text {
  margin-right: 6rpx;
  color: red;
}
/* 普通按钮点击背景颜色改变 */
.common-btn-hover {
  background: #f5f5f5;
}
/* 反馈状态 */
.status {
  font-size: 24rpx;
  color: #ffffff;
  border-radius: 20rpx;
  padding: 0 8rpx;
}
.status-0 {
  background-color: #007aff;
}
.status-1 {
  background-color: #ffb400;
}
.status-2 {
  background-color: #bebebe;
}
/* 超出两行省略 */
.two-line {
  color: #000000;
  width: 80%;
  line-height: 50rpx;
  max-height: 100rpx;
  text-overflow: -o-ellipsis-lastline;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
}
.text-overflow {
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
  display: inline-block;
}
.gray {
  color: rgba(0, 0, 0, 0.36);
}
/* 分隔线 */
.dividing-line {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #888888;
  letter-spacing: 0px;
  text-align: left;
  padding: 4px;
  background: #f5f5f5;
}
/* 记录列表样式 */
.list-view-container {
  width: 96%;
  margin: 0 auto;
}
.list-view-container .list-ul-container {
  list-style: none;
  padding: 0;
}
.list-view-container .list-ul-container .list-item-container {
  border: 1rpx solid #d9d9d9;
  border-radius: 15rpx;
  padding: 10rpx;
  margin: 20rpx auto;
}
.list-view-container .list-ul-container .list-item-container .list-item {
  width: 100%;
  height: auto;
  min-height: 60rpx;
  line-height: 100%;
  display: inline-flex;
}
.list-view-container .list-ul-container .list-item-container .list-item .left-name {
  display: inline-block;
  width: 30%;
  color: #aaaaaa;
  margin-top: 3%;
}
.list-view-container .list-ul-container .list-item-container .list-item .right-info {
  display: inline-block;
  width: 70%;
  margin-top: 3%;
}
.list-view-container .list-ul-container .list-item-container .list-item .top-title {
  width: 85%;
}
.list-view-container .list-ul-container .list-item-container .list-item .status-tab {
  width: 13%;
  margin-left: 2%;
  float: right;
  color: #ffffff;
  background-color: #d1d1d1;
  border-radius: 20rpx;
  padding: 0 6rpx;
  height: 50rpx;
  text-align: center;
  font-size: 26rpx;
}
.list-view-container .list-ul-container .list-item-container .list-item .status-btn {
  width: 13%;
  margin-left: 2%;
  float: right;
  color: #1e90ff;
  border-radius: 20rpx;
  padding: 0 6rpx;
  height: 50rpx;
  text-align: center;
  font-size: 26rpx;
  border: 1rpx solid #1e90ff;
}
.list-view-container .list-ul-container .list-item-container .top-title-item {
  padding: 8rpx 0;
  border-bottom: 1rpx solid #dddddd;
}
/* 详情页面列表样式(左右分布式) */
.info-list {
  background: #ffffff;
  min-height: 100rpx;
  height: auto;
  width: 94%;
  display: inline-flex;
  margin-left: 3%;
  margin-right: 3%;
  margin-bottom: 4rpx;
  border-bottom: 2rpx solid #eeeeee;
}
.left-cnt {
  width: 30%;
  text-align: left;
  display: inline-block;
  margin-top: 3%;
  color: gray;
}
.right-cnt {
  width: 62%;
  padding-left: 18rpx;
  text-align: left;
  display: inline-block;
  margin-top: 3%;
}
.require-text {
  color: red;
}
.hover-class {
  opacity: 0.6;
}
.info {
  width: 100%;
}
.info .info-content {
  width: 100%;
}
.info .info-content .top-item-container {
  border-top: 2rpx solid #CCCCCC;
}
.info .info-content .info-item {
  padding: 20rpx;
  overflow: hidden;
  border-bottom: 2rpx solid #CCCCCC;
}
.info .info-content .info-item input {
  font-size: 30rpx !important;
}
.info .info-content .get-info-btn,
.info .info-content .set-info-btn,
.info .info-content .operation-btn {
  padding: 30rpx 20rpx 0;
}
.wrap {
  background-color: #FFFFFF;
}
.list_li {
  border-bottom: 1px solid #DCDFE6;
  padding: 20rpx 10rpx;
}
.wifi-list {
  background: #FFFFFF;
}
.wifi-list ._ul {
  padding: 0;
}
.wifi-list ._ul ._li {
  background: #FFFFFF;
  height: 80rpx;
  line-height: 80rpx;
  width: 96%;
  padding: 0 10%;
  border-bottom: 2rpx solid #eee;
}


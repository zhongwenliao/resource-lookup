
.dev-list {
	width: 98%;
	margin: 0 1%;
}
.dev-list ._ul {
	padding: 0;
}
.dev-list ._ul ._li {
	width: 96%;
	padding: 20rpx 2%;
	overflow: hidden;
	border-bottom: 1rpx solid #bebebe;
}
.left-info {
	width: 50%;
	float: left;
}
.middle-info {
	width: 30%;
	float: left;
}
.right-info {
	width: 20%;
	float: left;
	color: #007AFF;
	text-align: right;
}


}
.list-view {
  width: 100%;
  background: #eeeeee;
}
.list-view .search-view {
  background: #eeeeee;
  line-height: 1;
  position: fixed;
  z-index: 1000;
  width: 100%;
  height: 100rpx;
}
.list-view .search-view .search-form {
  width: calc(100% - 60rpx);
  margin: 20rpx 30rpx;
  display: inline-block;
  background: #ffffff;
  border-radius: 10rpx;
  height: 60rpx;
}
.list-view .search-view .search-form .filter-view {
  posotion: relative;
}
.list-view .search-view .search-form .filter-view .filter-picker {
  top: 20rpx;
  height: 60rpx;
  padding: 8rpx 14rpx;
  border: 1rpx solid #eeeeee;
  border-radius: 10rpx;
  position: absolute;
  width: 90%;
}
.list-view .search-view .search-form .filter-view .filter-picker .filter-view-down-image {
  width: 40rpx;
  height: 30rpx;
  top: 20rpx;
  left: 90%;
  position: absolute;
}
.list-view .search-view .search-form .filter-view .filter-picker .input-text {
  white-space: nowrap;
  max-width: 90%;
  display: inline-block;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
  height: 60rpx;
}
.list-view .search-view .search-form .filter-view .filter-input {
  top: 24rpx;
  position: absolute;
  margin-left: 42%;
  font: inherit;
  display: inline-block;
  width: 40%;
}
.list-view .search-view .search-form .search-button::after {
  border: none;
}
.list-view .search-view .search-form .search-button {
  display: inline;
  background: none;
  margin: -20rpx 0 0 100%;
  padding: 12rpx 20rpx;
  float: left;
}
.list-view .search-view .search-form .search-image {
  width: 40rpx;
  margin: 0;
  height: 40rpx;
}
.list-view .no-data-view {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
  color: #959595;
}
.list-view .loading {
  padding: 30rpx 0;
  color: #959595;
  text-align: center;
}
.uni-base-page {
  width: 100%;
  height: 100%;
}
.red-help-text {
  color: red;
}
.bottom-btn-view {
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 100000000;
}
.btn {
  border-radius: 0px;
  font-size: 32rpx;
}
.bottom-btn-view .btn::after {

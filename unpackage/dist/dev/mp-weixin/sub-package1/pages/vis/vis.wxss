.content {
  padding: 0;
  background-color: #F2F2F2;
}
.vis-tab {
  background: #fff;
  padding-top: 20px;
}
.taberr {
  display: block;
  width: 230rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #000;
  font-size: 32rpx;
  margin: auto;
}
.tabactive {
  display: block;
  width: 230rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #0FAEFF;
  border-bottom: 5rpx solid #0FAEFF;
  font-size: 32rpx;
  margin: auto;
}
.vis-history-btn {
  width: 95%;
  margin: 0 auto;
  padding: 10rpx 0;
  font-size: 26rpx;
  text-align: right;
}
.btn-text {
  float: right;
  color: #0FAEFF;
  background: transparent;
  font-size: 26rpx;
  width: 30%;
  border-width: 0rpx;
  text-align: right;
}
.vis-info {
  background-color: #FFFFFF;
  padding: 20rpx 0;
  margin: 0 auto;
  margin-top: 20rpx;
}
.vis-times {
  background-color: #ffffff;
  padding: 0;
  margin-top: 20rpx;
}
.content ._ul {
  padding: 0;
  list-style: none;
}
.vis-info ._ul ._li {
  background: #FFFFFF;
  word-break: break-all;
  min-height: 100rpx;
  height: auto;
  width: 94%;
  margin-left: 3%;
  margin-right: 3%;
  margin-bottom: 4rpx;
  position: relative;
}
.vis-info ._ul ._li:before {
  content: '';
  position: absolute;
  bottom: 32rpx;
  right: 36rpx;
  height: 2rpx;
  width: calc(100% - 180rpx);
  background-color: #dddddd;
}
.content image {
  width: 32rpx;
  height: 32rpx;
}
.left-icon {
  width: 180rpx;
  float: left;
  text-align: left;
  margin-top: 3%;
}
.left-icon image {
  display: inline-block;
  vertical-align: middle;
  margin-bottom: 2rpx;
  margin-right: 8rpx;
}
.middle {
  display: inline-block;
  margin-top: 3%;
  padding-right: 30rpx;
  overflow: auto;
  clear: both;
}
.middle text {
  color: #666666;
}
.middle input {
  display: inline-block;
}
.right {
  width: 10%;
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  vertical-align: middle;
  text-align: right;
}
.right image {
  display: inline-block;
  vertical-align: middle;
}

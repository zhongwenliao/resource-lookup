.form-view .require-text {
  margin-right: 6rpx;
  color: red;
}
/* 普通按钮点击背景颜色改变 */
.common-btn-hover {
  background: #f5f5f5;
}
/* 反馈状态 */
.status {
  font-size: 24rpx;
  color: #ffffff;
  border-radius: 20rpx;
  padding: 0 8rpx;
}
.status-0 {
  background-color: #007aff;
}
.status-1 {
  background-color: #ffb400;
}
.status-2 {
  background-color: #bebebe;
}
/* 超出两行省略 */
.two-line {
  color: #000000;
  width: 80%;
  line-height: 50rpx;
  max-height: 100rpx;
  text-overflow: -o-ellipsis-lastline;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
}
.text-overflow {
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
  display: inline-block;
}
.gray {
  color: rgba(0, 0, 0, 0.36);
}
/* 分隔线 */
.dividing-line {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #888888;
  letter-spacing: 0px;
  text-align: left;
  padding: 4px;
  background: #f5f5f5;
}
/* 记录列表样式 */
.list-view-container {
  width: 96%;
  margin: 0 auto;
}
.list-view-container .list-ul-container {
  list-style: none;
  padding: 0;

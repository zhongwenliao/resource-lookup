
page {
	background-color: #efeff4;
}
.wrap {
	padding: 18rpx 0;
}
.wifi-setting ._ul {
	padding: 0
}
.wifi-setting ._ul ._li {
	background: #ffffff;
	min-height: 80rpx;
	height: auto;
	display: inline-flex;
	line-height: 80rpx;
	width: 96%;
	padding: 0 2%;
	border-bottom: 2rpx solid #eee;
}
.wifi-setting .manual-mode-btn {
	padding: 10rpx 23rpx;
	color: #1E90FF;
	margin: 8rpx 0;
	text-align: right;
}
.wifi-setting .manual-mode-btn-hover {
	background-color: #D3D3D3;
}
.info-title {
	width: 30%;
	display: inline-block;
	padding: 10rpx;
	line-height: 80rpx;
}
.info-name {
	width: 70%;
	display: inline-block;
	text-align: right;
	padding: 10rpx;
	line-height: 80rpx;
	color: grey;
}
.info-name input {
	font-size: 28rpx;
	height: 80rpx;
	line-heigth: 80rpx;
}
.setting-button {
	width: 100%;
}
.setting-button button {
	width: 80%;
	margin: 40rpx auto;
}


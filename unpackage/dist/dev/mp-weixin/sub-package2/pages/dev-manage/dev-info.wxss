.dev-info ._ul {
  padding: 0;
}
.dev-info ._li {
  background: #ffffff;
  min-height: 80rpx;
  height: auto;
  display: inline-flex;
  line-height: 80rpx;
  width: 96%;
  padding: 0 2%;
  border-bottom: 2rpx solid #eee;
}
.dev-info ._li .info-title {
  width: 40%;
  display: inline-block;
  padding: 10rpx;
  line-height: 80rpx;
}
.dev-info ._li .info-name {
  width: 60%;
  display: inline-block;
  text-align: right;
  padding: 10rpx;
  line-height: 80rpx;
}
.dev-info ._li .info-name input {
  height: 80rpx;
  line-heigth: 80rpx;
}
.dev-info ._li .select-li {
  width: 98%;
  padding: 10rpx;
  line-height: 80rpx;
  display: inline-block;
}
.dev-info ._li .select-li::after {
  float: right;
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  position: relative;
  top: 30rpx;
  margin-left: .3em;
}
.dev-info button {
  width: 80%;
  margin: 80rpx auto 0;
}
.dev-info .dividing-line {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #888888;
  letter-spacing: 0px;
  text-align: left;
  padding: 4px;
  background: #eeeeee;
}

